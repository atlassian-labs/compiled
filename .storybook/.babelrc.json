{
  "plugins": [
    [
      "@compiled/babel-plugin",
      {
        "importReact": false,
        "optimizeCss": false,
        "transformerBabelPlugins": [
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ]
        ]
      }
    ]
  ],
  "presets": ["@babel/preset-typescript", ["@babel/preset-react", { "runtime": "automatic" }]]
}
