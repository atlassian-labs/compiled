[ignore]

[include]
packages/.*/*.js.flow

[libs]

[declarations]
<PROJECT_ROOT>/flow_typed/.*
.*/node_modules/.*

[lints]
deprecated-utility=warn
unnecessary-optional-chain=warn
implicit-inexact-object=error
untyped-type-import=error

[options]
exact_by_default=true

[strict]
deprecated-type
deprecated-utility
nonstrict-import
sketchy-null
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
