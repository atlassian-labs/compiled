    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-jest": "24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.2",
    "ts-node-dev": "1.0.0-pre.44",
    "ts-transformer-testing-library": "^1.0.0-alpha.7",
    "ttypescript": "^1.5.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
