{
  "name": "@untitled/css-in-js-project",
  "version": "0.0.0",
  "description": "Compile away your css in js problems. Made for Component Libraries.",
  "keywords": [
    "css-in-js"
  ],
  "author": "Michael Dougall",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npx nodemon --exec \"yarn build && start-storybook -p 6006 --ci\" --watch src/ts-transformer/ -e tsx",
    "start:inspect": "npx nodemon --exec \"yarn build && node --inspect-brk node_modules/.bin/start-storybook -p 6006 --ci\" --watch src/ts-transformer/ -e tsx",
    "test": "jest",
    "test:cover": "jest --collectCoverage",
    "build": "tsc --module commonjs",
    "build:inspect": "node --inspect-brk node_modules/typescript/lib/tsc.js --module commonjs",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "stylis": "^3.5.4",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "24.0.19",
    "@types/node": "^12.12.17",
    "babel-loader": "^8.0.6",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "jest-extended": "^0.11.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-jest": "24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.2",
    "ts-node-dev": "1.0.0-pre.44",
    "ts-transformer-testing-library": "^1.0.0-alpha.4",
    "ttypescript": "^1.5.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "bracketSpacing": true,
    "semi": true,
    "tabWidth": 2,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  }
}
