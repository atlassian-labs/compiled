{
  "name": "@compiled/cli",
  "version": "0.0.1",
  "description": "Command-line commands for analysing files generated by the Compiled library.",
  "license": "Apache-2.0",
  "author": "Grant Wong",
  "bin": {
    "compiled-cli": "./bin/run"
  },
  "main": "dist/index.js",
  "repository": "atlassian-labs/compiled",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@compiled/css": "^0.11.0",
    "@oclif/core": "^2",
    "@oclif/plugin-autocomplete": "^2.1.9",
    "@oclif/plugin-help": "^5"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "eslint": "^8.41.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "compiled-cli",
    "dirname": "cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && shx rm tsconfig.tsbuildinfo && tsc -b",
    "posttest": "yarn lint",
    "pack": "yarn build && oclif manifest && oclif readme && shx rm -f oclif.manifest.json",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/atlassian-labs/compiled/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
