{
  "name": "@compiled/react",
  "version": "0.9.1",
  "description": "A familiar and performant compile time CSS-in-JS library for React.",
  "keywords": [
    "compiled",
    "css-in-js",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://compiledcssinjs.com",
  "bugs": "https://github.com/atlassian-labs/compiled/issues/new?assignees=&labels=bug&template=bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/compiled.git",
    "directory": "packages/react"
  },
  "license": "Apache-2.0",
  "author": "Michael Dougall",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": [
        "./dist/esm/index.js",
        "./src/index.tsx"
      ],
      "require": [
        "./dist/cjs/index.js",
        "./src/index.tsx"
      ]
    },
    "./runtime": {
      "import": [
        "./dist/esm/runtime.js",
        "./src/runtime.tsx"
      ],
      "require": [
        "./dist/cjs/runtime.js",
        "./src/runtime.tsx"
      ]
    },
    "./jsx-runtime": {
      "import": "./dist/esm/jsx/jsx-runtime.js",
      "require": "./dist/cjs/jsx/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/esm/jsx/jsx-dev-runtime.js",
      "require": "./dist/cjs/jsx/jsx-dev-runtime.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/browser/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src",
    "runtime",
    "jsx-runtime",
    "jsx-dev-runtime"
  ],
  "dependencies": {
    "csstype": "^3.0.9"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/react-dom": "^17.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">= 16.12.0"
  }
}
