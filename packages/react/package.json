{
  "name": "@compiled/react",
  "version": "0.17.0",
  "description": "A familiar and performant compile time CSS-in-JS library for React.",
  "keywords": [
    "compiled",
    "css-in-js",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://compiledcssinjs.com/docs/pkg-react",
  "bugs": "https://github.com/atlassian-labs/compiled/issues/new?assignees=&labels=bug&template=bug_report.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/compiled.git",
    "directory": "packages/react"
  },
  "license": "Apache-2.0",
  "author": "Michael Dougall",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": [
        "./dist/esm/index.js",
        "./src/index.ts"
      ],
      "require": [
        "./dist/cjs/index.js",
        "./src/index.ts"
      ]
    },
    "./runtime": {
      "import": [
        "./dist/esm/runtime.js",
        "./src/runtime.ts"
      ],
      "require": [
        "./dist/cjs/runtime.js",
        "./src/runtime.ts"
      ]
    },
    "./jsx-runtime": {
      "import": [
        "./dist/esm/jsx/jsx-runtime.js",
        "./src/jsx/jsx-runtime.ts"
      ],
      "require": [
        "./dist/cjs/jsx/jsx-runtime.js",
        "./src/jsx/jsx-runtime.ts"
      ]
    },
    "./jsx-dev-runtime": {
      "import": [
        "./dist/esm/jsx/jsx-dev-runtime.js",
        "./src/jsx/jsx-dev-runtime.ts"
      ],
      "require": [
        "./dist/cjs/jsx/jsx-dev-runtime.js",
        "./src/jsx/jsx-dev-runtime.ts"
      ]
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/browser/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src",
    "runtime",
    "jsx-runtime",
    "jsx-dev-runtime"
  ],
  "dependencies": {
    "csstype": "^3.1.2"
  },
  "devDependencies": {
    "@compiled/benchmark": "^1.1.0",
    "@fixture/strict-api-test": "*",
    "@testing-library/react": "^12.1.5",
    "@types/jsdom": "^16.2.15",
    "@types/react-dom": "^17.0.22",
    "expect-type": "^0.17.3",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">= 16.12.0"
  }
}
