{
  "extends": "../tsconfig.options.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "@compiled/style": ["index.tsx"]
    },
    "plugins": [
      {
        "transform": "ts-transform-define",
        "replace": {
          "typeof window": "process.env.TYPEOF_WINDOW"
        }
      }
    ]
  },
  "references": []
}
